<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SpecFlow.XForms.DemoApp.AnotherPage">

  <ContentPage.Content>
    <StackLayout Padding="10,20,10,10" HorizontalOptions="CenterAndExpand" Spacing="10" Orientation="Vertical" x:Name="RelLayout">
      <Label Text="SpecFlow.XamarinForms AnotherPage" FontSize="18" HorizontalTextAlignment="Center" />

      <BoxView HeightRequest="30" BackgroundColor="Transparent"></BoxView>

      <Button Text="Back Button" Command="{Binding BackCommand}" BorderColor="Gray" BorderWidth="2">
      </Button>

      <BoxView HeightRequest="30"  BackgroundColor="Transparent"></BoxView>
    </StackLayout>
  </ContentPage.Content>

</ContentPage>
